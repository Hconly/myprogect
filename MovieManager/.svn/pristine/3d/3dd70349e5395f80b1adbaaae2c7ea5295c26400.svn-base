package entity;
/**
 * 影厅类
 * @author Administrator
 *
 */
public class Table_Hall {
	private int hid;//影厅编号
	private int hseat;//影厅座位
	private String hdesc;//影厅描述
	private String haddress;//影厅地址
	public int getHid() {
		return hid;
	} 
	public void setHid(int hid) {
		this.hid = hid;
	}
	public int getHseat() {
		return hseat;
	}
	public void setHseat(int hseat) {
		this.hseat = hseat;
	}
	public String getHdesc() {
		return hdesc;
	}
	public void setHdesc(String hdesc) {
		this.hdesc = hdesc;
	}
	public String getHaddress() {
		return haddress;
	}
	public void setHaddress(String haddress) {
		this.haddress = haddress;
	}
	

}
